#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int dp[100][100]={0};
int helper(string s1,int m,int n)
{
    if(m==0 || n==0)return dp[m][n] = 0;
    if(dp[m][n]!=0)return dp[m][n];
    if(s1[m-1]==s1[n-1] && m!=n)
        return dp[m][n] = 1+helper(s1,m-1,n-1);

    return dp[m][n] = max(helper(s1,m-1,n),helper(s1,m,n-1));
;}
int main()
{
    string s1 = "aabb";

    cout<<helper(s1,s1.length(),s1.length());
}
