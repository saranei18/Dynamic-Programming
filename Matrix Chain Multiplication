#include<iostream>
#include<algorithm>
#include<vector>
#include<string>
#include<climits>
using namespace std;

int helper(vector<int>&arr,int i,int j)
{
    if(i==j)return 0;
    int ans=INT_MAX;

    int counts=0;
    for(int l=i;l<j;l++)
    {
        counts=helper(arr,i,l)+helper(arr,l+1,j)+arr[i-1]*arr[l]*arr[j];

        ans=min(ans,counts);
    }
    return ans;
}
int main()
{
    vector<int>arr{1,2,3,4,3};
    cout<<helper(arr,1,arr.size()-1);
}
