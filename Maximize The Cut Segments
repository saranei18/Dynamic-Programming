#include<iostream>
#include<algorithm>
#include<vector>
#include<string>
#include<climits>
#include<cstring>
using namespace std;

int main()
{
    int n=5;
    int a[3]={5,3,2};
    int dp[n+1];
    for(int i=0;i<=n;i++)dp[i]=0;
    for(int j=0;j<3;j++)
    {
        for(int i=1;i<=n;i++)
        {
            if(i==a[j] || (i>a[j] && dp[i-a[j]]>0))
                dp[i]=max(dp[i-a[j]]+1,dp[i]);
        }
    }
    cout<<dp[n];
    return 0;
}
