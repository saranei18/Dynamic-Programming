#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;


int main()
{
    vector<int>nums {1, 101, 2, 3, 100, 4, 5};
    int n=nums.size();
    int lis[n];
    for(int i=0;i<n;i++)lis[i]=nums[i];
    for(int i=1;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(nums[i]>nums[j] && lis[i]<lis[j]+nums[i])
                lis[i]=lis[j]+nums[i];
        }
    }
    for(int i=0;i<n;i++)cout<<"  "<<lis[i];
    cout<<endl;
    int ans=0;
    for(int i=0;i<n;i++)
        ans=max(ans,lis[i]);

    cout<<ans;
    return 0;
}
