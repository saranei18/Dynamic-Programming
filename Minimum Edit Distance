#include<iostream>
#include<algorithm>
#include<vector>
#include<string>
#include<climits>
using namespace std;

int dp[10][10]={0};
int helper(string s1,string s2,int m,int n)
{
    if(m==0)return n;
    if(n==0)return m;
    if(dp[m][n]!=0)return dp[m][n];
    if(s1[m-1]==s2[n-1])
        return dp[m][n] = helper(s1,s2,m-1,n-1);
    return dp[m][n] = 1+min(helper(s1,s2,m,n-1),min(helper(s1,s2,m-1,n),helper(s1,s2,m-1,n-1)));
}

int main()
{
    string s1="sunday";
    string s2="saturday";
    cout<<helper(s1,s2,s1.length(),s2.length());
}
