#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
#include<climits>
using namespace std;
int main()
{
    vector<int>nums{1 ,4 ,3 ,2 ,6 ,7};
    int n=nums.size();
    int dp[n];
    for(int i=0;i<n;i++)dp[i]=INT_MAX;
    dp[0]=0;
    for(int i=1;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(dp[j]!=INT_MAX && nums[j]+j>=i)
            {
                if(dp[j]+1<dp[i])
                    dp[i]=dp[j]+1;
            }
        }
    }
    cout<<dp[n-1];
    return 0;

}



O(n):
-----------------


#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
#include<climits>
using namespace std;
int main()
{
    vector<int>nums{1 ,4 ,3 ,2 ,6 ,7};
    int maxind=nums[0];
    int step=nums[0];
    int jump=1;
    for(int i=1;i<nums.size();i++)
    {
        if(i==nums.size()-1)
        {
            cout<<jump;
            return 0;
        }
        maxind=max(maxind,nums[i]+i);
        step--;
        if(step==0)
        {
            jump++;
            if(i>=maxind)return -1;
            step=maxind-i;
        }
    }

    return 0;
}
