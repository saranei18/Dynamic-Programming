#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
#include<climits>
using namespace std;
int main()
{
    vector<int>nums{1 ,4 ,3 ,2 ,6 ,7};
    int n=nums.size();
    int dp[n];
    for(int i=0;i<n;i++)dp[i]=INT_MAX;
    dp[0]=0;
    for(int i=1;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(dp[j]!=INT_MAX && nums[j]+j>=i)
            {
                if(dp[j]+1<dp[i])
                    dp[i]=dp[j]+1;
            }
        }
    }
    cout<<dp[n-1];
    return 0;

}
