#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
using namespace std;

int main()
{
    int n=3;
    int c[n+1];
    for(int i=0;i<=n;i++)c[i]=0;
    c[0]=c[1]=1;

    for(int i=2;i<=n;i++)
    {
        for(int j=0;j<i;j++)
            c[i]+=c[j]*c[i-j-1];
    }
    cout<<c[n];
}
